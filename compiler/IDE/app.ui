<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="tk.Menu" id="menu_1">
    <child>
      <object class="tk.Menuitem.Submenu" id="submenu_file">
        <property name="label" translatable="yes">Arquivo</property>
        <child>
          <object class="tk.Menuitem.Command" id="command_new">
            <property name="command">cb_menu_new</property>
            <property name="command_id_arg">false</property>
            <property name="label" translatable="yes">Novo arquivo [Ctrl-N]</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Separator" id="separator_1" />
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="command_open">
            <property name="command">cb_menu_open</property>
            <property name="command_id_arg">false</property>
            <property name="label" translatable="yes">Abrir arquivo [Ctrl-O]</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Separator" id="separator_2" />
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="command_save">
            <property name="command">cb_menu_save</property>
            <property name="command_id_arg">false</property>
            <property name="label" translatable="yes">Salvar arquivo [Ctrl-S]</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="command_saveas">
            <property name="command">cb_menu_saveas</property>
            <property name="command_id_arg">false</property>
            <property name="label" translatable="yes">Salvar como</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Menuitem.Submenu" id="submenu_compiler">
        <property name="label" translatable="yes">Compilador</property>
        <child>
          <object class="tk.Menuitem.Command" id="command_compile">
            <property name="command">cb_menu_compile</property>
            <property name="command_id_arg">false</property>
            <property name="label" translatable="yes">Compilar</property>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="ttk.Frame" id="frame_main">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="frame_ribbon">
        <property name="height">30</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame_ide">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Text" id="text_default">
            <property name="height">30</property>
            <property name="setgrid">false</property>
            <property name="tabstyle">tabular</property>
            <property name="width">100</property>
            <layout manager="pack">
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame_footer">
        <property name="height">10</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="fill">x</property>
          <property name="padx">10</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label_footer_col">
            <property name="text" translatable="yes">Col 0</property>
            <layout manager="pack">
              <property name="propagate">True</property>
              <property name="side">right</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Separator" id="separator_footer_1">
            <property name="orient">vertical</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="propagate">True</property>
              <property name="side">right</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label_footer_line">
            <property name="text" translatable="yes">Ln 0</property>
            <layout manager="pack">
              <property name="propagate">True</property>
              <property name="side">right</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
